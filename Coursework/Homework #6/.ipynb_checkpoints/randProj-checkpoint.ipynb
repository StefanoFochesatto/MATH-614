    "using LinearAlgebra\n",
    "using Random\n",
    "\n",
    "function randproj(m,k)\n",
    "#This function takes a dimension m, and a rank k\n",
    "#and produces an orthogonal projector P, on a k\n",
    "#dimension subspace of m\n",
    "    A = randn(m);\n",
    "    [Q, R] = qr(A);\n",
    "    P = Q(:,1:k)*Q(:,1:k)';\n",
    "    return P\n",
    "end\n"
